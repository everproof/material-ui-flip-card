{
  "name": "material-ui-flip-card",
  "version": "0.2.2",
  "description": "An extension of material-ui's Card to allow it to flip.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:lib": "babel -d lib src",
    "build:flow": "flow-copy-source src lib",
    "format": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -i 'src/**/*.jsx' -t html -t json -t text --threshold 90",
    "lint": "eslint src --ext .js,.jsx",
    "test": "npm run format && npm run lint && flow && npm run flow:coverage && jest --coverage",
    "test:watch": "jest --watch",
    "precommit": "npm test",
    "prepush": "npm run precommit && npm run build",
    "prepublish": "npm run prepush"
  },
  "author": {
    "name": "Rhys Gevaux",
    "email": "rhys@everproof.com",
    "url": "https://twitter.com/vorksau"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/everproof/material-ui-flip-card.git"
  },
  "files": [
    "lib",
    "src"
  ],
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/es/**",
      "!**/coverage/**",
      "!**/flow-coverage/**",
      "!**/flow-typed/**",
      "!**/lib/**"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFiles": [
      "./test-setup.js"
    ]
  },
  "dependencies": {
    "classnames": "2.2.5",
    "material-ui": "1.0.0-beta.26",
    "prop-types": "15.6.0",
    "react": "15.6.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.4.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.5",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "flow-bin": "0.64.0",
    "flow-copy-source": "1.2.2",
    "flow-coverage-report": "0.4.1",
    "flow-typed": "2.3.0",
    "husky": "0.14.3",
    "jest": "22.1.4",
    "prettier-eslint-cli": "4.7.1",
    "react-dom": "15.6.2",
    "react-test-renderer": "15.6.2",
    "rimraf": "2.6.2"
  },
  "peerDependencies": {
    "material-ui": ">=1.0.0-beta.26",
    "prop-types": ">=15.0.0",
    "react": ">=15.0.0"
  }
}
