{
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "SyntheticEvent": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:react/recommended",
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "plugins": ["compat"],
  "rules": {
    "compat/compat": "error",
    "import/default": "error",
    "import/extensions": "off",
    "import/named": "error",
    "import/namespace": "error",
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "newlines-between": "always"
      }
    ],
    "import/prefer-default-export": "off",
    "react/jsx-key": "error",
    "react/jsx-no-duplicate-props": ["error", { "ignoreCase": false }],
    "react/no-typos": "off"
  }
}
