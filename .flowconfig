[ignore]
<PROJECT_ROOT>/buildClient/
<PROJECT_ROOT>/buildServer/
<PROJECT_ROOT>/coverage/
.*/node_modules/babel-plugin-flow-react-proptypes
.*/node_modules/babel-plugin-transform-react-remove-prop-types
.*/node_modules/editions
.*/node_modules/eslint-plugin-jsx-a11y
.*/node_modules/graphql
.*/node_modules/kefir
.*/node_modules/react-event-listener/src

.*/node_modules/material-ui/test-utils

.*/node_modules/jss/lib/.*\.js\.flow

.*/node_modules/react-swipeable-views
.*/node_modules/react-universal-component
.*/node_modules/redux-first-router-link
.*/node_modules/webpack-visualizer-plugin

[include]

[libs]
flow-typed

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
esproposal.decorators=ignore

module.ignore_non_literal_requires=true
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=app

suppress_type=$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
